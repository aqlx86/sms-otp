{
    "name": "aqlx86/sms-otp",
    "description": "SMS OTP for Laravel 5 using iSMS gateway.",
    "keywords": ["sms", "isms", "laravel", "otp"],
    "homepage": "https://github.com/aqlx86/sms-otp",
    "license": "MIT",
    "authors": [
        {
            "name": "Arnel Labarda",
            "email": "aqlx86@gmail.com",
            "homepage": "https://aql.ph/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "aqlx86/isms-php": "^1.2",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "SMSOTP\\": "src/SMSOTP/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "bin-dir": "bin"
    }
}
